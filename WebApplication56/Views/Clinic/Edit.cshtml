@model WebApplication56.Models.Clinic

<div class="container">
    <h2>Edit Clinic</h2>
    <form asp-action="Edit">
        <div class="form-group">
            <label asp-for="ClinicId" class="control-label"></label>
            <input asp-for="ClinicId" class="form-control" readonly />
        </div>
        <div class="form-group">
            <label asp-for="ClinicName" class="control-label"></label>
            <input asp-for="ClinicName" class="form-control" />
        </div>
        <div class="form-group">
            <input type="submit" value="Save" class="btn btn-primary" />
        </div>
    </form>
</div>
